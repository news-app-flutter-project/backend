{
  "name": "news-app-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "postinstall": "npm run build",
    "api-docs": "swagger-cli bundle ./src/swagger/openapi.yaml --outfile build/swagger.yaml --type yaml",
    "migrate:generate": "npx sequelize-cli migration:generate --name add-new-field-to-users",
    "migrate:dev": "npx sequelize-cli db:migrate --env development",
    "migrate:test": "npx sequelize-cli db:migrate --env test",
    "migrate:prod": "npx sequelize-cli db:migrate --env production",
    "migrate": "npm run migrate:dev && npm run migrate:test && npm run migrate:prod",
    "switch-node-env": "bash -c 'if [ \"$1\" == \"test\" ]; then export NODE_ENV=test; echo \"NODE_ENV switched to test\"; elif [ \"$1\" == \"prod\" ]; then export NODE_ENV=production; echo \"NODE_ENV switched to production\"; else export NODE_ENV=development; echo \"NODE_ENV switched to development\"; fi;' --"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "sequelize-cli": "^6.6.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "module-alias": "^2.2.2",
    "mysql2": "^3.2.0",
    "sequelize": "^6.30.0"
  },
  "_moduleAliases": {
    "@/utils": "dist/utils",
    "@/controllers": "dist/controllers",
    "@/middlewares": "dist/middlewares",
    "@/apis": "dist/apis",
    "@/swagger": "dist/swagger",
    "@/common": "dist/common",
    "@/database": "dist/database"
  }
}
